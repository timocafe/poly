reset
RATIO=1
set terminal postscript eps enhanced color solid font 16

set size ratio RATIO
set key top left

#set multiplot
set origin 0.0,0.0
set yrange [8:20]
set xrange [50:100]


set xtics 10
set mxtics 5

set ytics 2
set mytics 5

set xlabel "Latency [cycle]"
set ylabel "Throughput [cycle]"

