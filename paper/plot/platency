
reset
RATIO=1
set terminal postscript eps enhanced color font 16
#set size 0.90,0.70
set size ratio RATIO
set key top left

set xrange [0:5]

set ylabel "elapse time [cycle]"
set xlabel "unroll factor"
#set title "Latency measurement"

f1(x)=a1*x+b1
f2(x)=a2*x+b2
f3(x)=a3*x+b3

set style line 1  lc rgb '#CF2E31' pt 7 lw 2 #red
#NA
set style line 2 lc rgb '#3333FF' pt 7 lw 2 lt 1 # blue
set style line 3 lc rgb '#1D9A2F' pt 7 lw 2 lt 1 # green
set style line 4 lc rgb '#FF358B' pt 7 lw 2 lt 1 # pink
set style line 5 lc rgb '#01B0F0' pt 7 lw 2 lt 1 # light blue
set style line 6 lc rgb '#000000' pt 7 lw 2 lt 1 # black
set style line 7 lc rgb '#C03000' pt 7 lw 2 lt 1 # vermillon

fit f1(x) "mass/out_mass" using 1:14 via a1,b1
fit f2(x) "mass/out_mass" using 1:15 via a2,b2
fit f3(x) "mass/out_mass" using 1:16 via a3,b3

fit f1(x) "mass/out_mass" using 1:14 via a1,b1
set out "figures/platency.eps";plot f1(x)/100  ls 1 w l notitle
set out "figures/platency.eps";replot "mass/out_mass"  using 1:($14/100) ls 1 title 'vectorial add mnemonic'

set terminal pop
